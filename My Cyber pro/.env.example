# Server Configuration
PORT=3000
NODE_ENV=development

# Database
DB_PATH=./data/labs.db

# Docker
DOCKER_SOCKET=/var/run/docker.sock

# Session
SESSION_SECRET=cyber-lab-platform-secret-key-change-in-production

# Admin Account (First Time Setup)
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin123

# Lab Configuration
LAB_TIMEOUT=3600000
MAX_CONCURRENT_LABS=5
AUTO_RECOVERY=true

# AI Chatbot Configuration
# Options: gemini, openai, none
CHATBOT_AI_PROVIDER=gemini

# Get your Gemini API key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your-gemini-api-key-here

# Get your OpenAI API key from: https://platform.openai.com/api-keys
# OPENAI_API_KEY=your-openai-api-key-here

# Model selection
CHATBOT_MODEL=gemini-pro
# For OpenAI: gpt-3.5-turbo or gpt-4

# Response configuration
CHATBOT_TEMPERATURE=0.7
CHATBOT_MAX_TOKENS=500
